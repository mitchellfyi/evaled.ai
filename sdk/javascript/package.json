  },
  "keywords": [
    "evald",
    "telemetry",
    "agent",
    "monitoring",
    "observability"
  ],
  "author": "evald.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kellaionline/evald.ai.git",
    "directory": "sdk/javascript"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
